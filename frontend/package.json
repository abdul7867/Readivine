{
  "name": "readivine-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:auth": "node src/test/auth/runAuthTests.js",
    "test:auth:flow": "vitest --run src/test/auth/authFlow.test.js",
    "test:auth:routes": "vitest --run src/test/auth/routeProtection.test.js",
    "test:auth:api": "vitest --run src/test/auth/apiConfiguration.test.js",
    "test:integration": "vitest --run src/test/integration/",
    "test:coverage": "vitest --coverage",
    "validate-config": "node ../scripts/validate-config.js",
    "validate-auth": "node -e \"import('./src/utils/validateAuthConfig.js').then(m => m.validateAuthConfig())\"",
    "validate": "npm run validate-config"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "terser": "^5.44.0",
    "vite": "^7.1.2",
    "vitest": "^1.0.4"
  },
  "description": "Readivine - AI-powered README generator frontend application",
  "main": "eslint.config.js",
  "keywords": [
    "readivine",
    "readme",
    "ai",
    "generator",
    "github",
    "react"
  ],
  "author": "",
  "license": "ISC"
}
