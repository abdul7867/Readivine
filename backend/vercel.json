{
  "version": 2,
  "name": "readivine-backend",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/auth/github",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/auth/github/callback",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/auth/check",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/auth/logout",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/v1/auth/status",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/github/repos",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/github/analyze",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/v1/github/save-readme",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/v1/github/save-readme-branch",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/v1/templates",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/api/v1/readme",
      "dest": "/index.js",
      "methods": ["GET", "POST", "DELETE"]
    },
    {
      "src": "/api/v1/readme/check-drafts",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/v1/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"]
}
